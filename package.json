{
  "name": "resiliencia-react",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "generate-types": "yarn update-schema && apollo client:codegen --target=typescript --no-addTypename --localSchemaFile=graphql-schema.json --outputFlat=src/gql/types.ts",
    "start": "react-scripts start",
    "update-schema": "apollo client:download-schema --endpoint=http://localhost:4000/graphql graphql-schema.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.18",
    "formik": "^2.2.6",
    "graphql": "^15.5.0",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.2.0",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "@types/leaflet": "^1.7.0",
    "@types/lodash": "^4.14.169",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "apollo": "^2.33.2",
    "eslint": "^7.26.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "typescript": "^4.2.4"
  }
}
